{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "rootDir": "./",
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
    ".prisma/*": ["node_modules/.prisma/*"]
  },
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "skipLibCheck": true
  }
}

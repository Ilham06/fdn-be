// prisma/ratings.prisma

generator ratingsClient {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/ratings"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_MYSQL")
}


model Rating {
  id         String   @id @default(uuid())
  productId  Int
  rate       Decimal
  count      Int
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  deletedAt  DateTime?
}
